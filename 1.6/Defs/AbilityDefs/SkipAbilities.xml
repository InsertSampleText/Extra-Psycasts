    <!--Tornado-->
    <AbilityDef ParentName="PsycastBaseSkip">
        <defName>Tornado</defName>
        <label>tornado</label>
        <description>Use differential-temperature skipgates in the atmosphere to call forth heavy winds and generate a localized small tornado.</description>
        <level>5</level>
        <iconPath>UI/Abilities/SkipTornado</iconPath>
        <writeCombatLog>True</writeCombatLog>
        <canUseAoeToGetTargets>False</canUseAoeToGetTargets>
        <statBases>
