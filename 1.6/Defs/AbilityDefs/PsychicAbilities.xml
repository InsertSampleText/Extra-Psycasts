    <!--Bolster
    Immunity-->
    <AbilityDef ParentName="PsycastBase">
        <defName>EP_BolsterImmunity</defName>
        <label>bolster immunity</label>
        <description>Psychically energize and reinforce the target's immune system, boosting their
            immunity gain.</description>
