<Defs>
    <!--Rock Throw-->
    <ThingDef ParentName="BaseBullet">
        <defName>SkippedRocks</defName>
        <label>skipped rocks</label>
        <graphicData>
            <texPath>Things/Projectile/SkippedRocks</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <projectile>
            <damageDef>Blunt</damageDef>
            <damageAmountBase>18</damageAmountBase>
            <armorPenetrationBase>.1</armorPenetrationBase>
            <stoppingPower>2.5</stoppingPower>
            <speed>55</speed>
        </projectile>
    </ThingDef>

    <!--Fireball-->
    <ThingDef ParentName="EtherealThingBase">
        <defName>EP_DustBomb</defName>
        <label>fine dust particles</label>
        <thingClass>ThingWithComps</thingClass>
        <tickerType>Normal</tickerType>

        <graphicData>
            <graphicClass>Graphic_Single</graphicClass>
            <texPath>Things/Mote/null</texPath>
        </graphicData>

        <comps>
            <li Class="CompProperties_Explosive">
                <explosiveDamageType>Bomb</explosiveDamageType>
                <explosiveRadius>3.9</explosiveRadius>
                <chanceToStartFire>.9</chanceToStartFire>
                <damageFalloff>true</damageFalloff>
                <explodeOnDestroyed>true</explodeOnDestroyed>
                <preExplosionSpawnSingleThingDef>Filth_BlastMark</preExplosionSpawnSingleThingDef>
            </li>

            <li Class="CompProperties_DestroyAfterDelay">
                <delayTicks>5</delayTicks>
            </li>
        </comps>
    </ThingDef>


    <!--Spark-->
    <ThingDef ParentName="EtherealThingBase">
        <defName>EP_Spark</defName>
        <label>spark</label>
        <thingClass>ThingWithComps</thingClass>
        <tickerType>Normal</tickerType>

        <graphicData>
            <graphicClass>Graphic_Single</graphicClass>
            <texPath>Things/Mote/null</texPath>
        </graphicData>

        <comps>
            <li Class="CompProperties_Explosive">
                <explosiveDamageType>Flame</explosiveDamageType>
                <explosiveRadius>.9</explosiveRadius>
                <chanceToStartFire>1</chanceToStartFire>
                <damageFalloff>true</damageFalloff>
                <explodeOnDestroyed>true</explodeOnDestroyed>
            </li>

            <li Class="CompProperties_DestroyAfterDelay">
                <delayTicks>5</delayTicks>
            </li>
        </comps>
    </ThingDef>


</Defs>