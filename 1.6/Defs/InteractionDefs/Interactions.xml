<Defs>

    <!--Word of Happiness-->
    <InteractionDef>
        <defName>EP_PsychicHappiness</defName>
        <label>psychic happiness</label>
        <workerClass>InteractionWorker</workerClass>
        <ignoreTimeSinceLastInteraction>True</ignoreTimeSinceLastInteraction>
        <symbol>Things/Mote/SpeechThings/WordOfHappinessConvo</symbol>
        <logRulesInitiator>
            <rulesStrings>
                <li>r_logentry->[INITIATOR_nameDef] psychically gave joy to [RECIPIENT_nameDef].</li>
            </rulesStrings>
        </logRulesInitiator>
    </InteractionDef>

